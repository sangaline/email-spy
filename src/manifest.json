{
  "name": "Email Spy",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "https://duckduckgo.com/*"
  ]
}
